{
  "name": "todo-react-ts",
  "version": "0.0.1",
  "description": "React project with TypeScript and WebPack",
  "private": true,
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "node react-ts -- serve 8080",
    "build": "node react-ts -- build",
    "build:dev": "node react-ts -- build development",
    "lint": "tslint src/**/*.tsx",
    "tdd": "concurrently -r 'npm run watch' 'npm run test:watch'",
    "test": "npm run compile && istanbul cover ./node_modules/mocha/bin/_mocha -- dist/**/*.spec.js",
    "test:watch": "nodemon --delay 1 --watch dist --exec 'istanbul cover ./node_modules/mocha/bin/_mocha -- dist/**/*.spec.js'",
    "compile": "tsc",
    "watch": "npm run compile -- -w"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.21",
    "awesome-typescript-loader": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "command-exists": "^1.2.2",
    "commander": "^2.9.0",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "name-util": "^1.0.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "progress": "^2.0.0",
    "react-dev-utils": "^0.5.2",
    "sass-loader": "^6.0.3",
    "seamless-immutable": "^7.1.2",
    "shelljs": "^0.7.7",
    "style-loader": "^0.16.1",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "reflect-metadata": "^0.1.10",
    "statex": "^1.0.1"
  }
}